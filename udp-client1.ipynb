    "import socket\n",
    "\n",
    "sock = socket.socket()\n",
    "sock.connect(('localhost', 3333))\n",
    "sock.send(b'hello, world!')\n",
    "\n",
    "data = sock.recv(1024)\n",
    "sock.close()\n",
    "\n",
    "print(data)"
